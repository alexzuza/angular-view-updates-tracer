{
  "manifest_version": 2,
  "name": "Angular Component Overview",
  "version": "2.0",
  "content_scripts": [
    {
      "matches": ["http://localhost/*"],
      "js": ["js/content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "web_accessible_resources": ["js/tracer.js"],
  "permissions": ["storage"]
}
